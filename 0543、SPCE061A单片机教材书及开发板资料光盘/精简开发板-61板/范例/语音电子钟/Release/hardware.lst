0000939B 09 B3 C0 FF        	                r1 &= ~0x003F;
0000939D 0A 5E              	                jz      L_RD_End;
                            	L_RD_Loop:                
0000939E 40 F0 E0 93        	                call    F_Delay;        
000093A0 41 94              	                r2 = 0x0001;
000093A1 1A D5 12 70        	                [P_Watchdog_Clear] = r2;
000093A3 09 23 40 00        	                r1 -= 0x40;
000093A5 19 D3 17 70        	                [P_DAC1] = r1;  
000093A7 4A 4E              	                jnz     L_RD_Loop;
                            	L_RD_End:       
                            	                
000093A8 90 90              	                pop     r1,r2 from [sp];
000093A9 90 9A              	                retf;
                            					.ENDP
                            	
