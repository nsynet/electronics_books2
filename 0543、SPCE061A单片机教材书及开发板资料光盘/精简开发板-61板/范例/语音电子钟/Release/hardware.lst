                            	_SP_RampUpDAC1:	.PROC
                            	F_SP_RampUpDAC1:
00009374 90 D4              	                push r1,r2 to [sp];
00009375 11 93 17 70        	                r1=[P_DAC1];
00009377 09 B3 C0 FF        	                r1 &= ~0x003f;
00009379 09 43 00 80        	                cmp     r1,0x8000
0000937B 0E 0E              	                jb     	L_RU_NormalUp;  
0000937C 19 5E              	                je      L_RU_End;
                            	                
                            	L_RU_DownLoop:
0000937D 40 F0 E0 93        	                call    F_Delay;        
0000937F 41 94              	                r2 = 0x0001;
00009380 1A D5 12 70        	                [P_Watchdog_Clear] = r2;
00009382 09 23 40 00        	                r1 -= 0x40;
00009384 19 D3 17 70        	                [P_DAC1] = r1;
00009386 09 43 00 80        	                cmp     r1,0x8000;
00009388 4C 4E              	                jne     L_RU_DownLoop;  
                            	L_RD_DownEnd:
00009389 0C EE              	                jmp     L_RU_End;
                            	
                            	L_RU_NormalUp:
                            	L_RU_Loop:
0000938A 40 F0 E0 93        	                call    F_Delay;
0000938C 41 94              	                r2 = 0x0001;
0000938D 1A D5 12 70        	                [P_Watchdog_Clear] = r2;
0000938F 09 03 40 00        	                r1 += 0x40;
00009391 19 D3 17 70        	                [P_DAC1] = r1;
00009393 09 43 00 80        	                cmp     r1, 0x8000;
00009395 4C 4E              	                jne     L_RU_Loop;
                            	                
                            	
                            	L_RU_End:
00009396 90 90              	                pop     r1,r2 from [sp];
00009397 90 9A              	                retf;
                            	                .ENDP
                            	/////////////////////////////////////// 
                            	_SP_RampDnDAC1:	.PROC
                            	F_SP_RampDnDAC1:
00009398 90 D4              					push r1,r2 to [sp];
