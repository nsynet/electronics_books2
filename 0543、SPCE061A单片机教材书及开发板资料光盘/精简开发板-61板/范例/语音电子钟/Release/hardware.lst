                            	_SP_Init_IOA: .PROC
0000933F 88 DA              			PUSH BP,BP TO [SP];
00009340 08 0B 01 00        	        BP = SP + 1;
00009342 88 D2              			PUSH R1,R1 TO [SP];
00009343 03 92              			R1 = [BP+3];						// Port direction
00009344 19 D3 02 70        			[P_IOA_Dir] = R1;
00009346 04 92              			R1 = [BP+4];
00009347 19 D3 00 70        			[P_IOA_Data] = R1;
00009349 05 92              			R1 = [BP+5];
0000934A 19 D3 03 70        			[P_IOA_Attrib] = R1;
                            			
0000934C 88 90              			POP R1,R1 FROM [SP];
0000934D 88 98              	        POP BP,BP FROM [SP];
0000934E 90 9A              	        RETF;
                            			.ENDP
                            	
                            	////////////////////////////////////////////////////////////////// 
                            	// SP_Inti_IOB
                            	//////////////////////////////////////////////////////////////////    
                            	
                            	_SP_Init_IOB: .PROC
0000934F 88 DA              			PUSH BP,BP TO [SP];
00009350 08 0B 01 00        	        BP = SP + 1;
00009352 88 D2              			PUSH R1,R1 TO [SP];
00009353 03 92              			R1 = [BP+3];						// Port direction
00009354 19 D3 07 70        			[P_IOB_Dir] = R1;
00009356 04 92              			R1 = [BP+4];
00009357 19 D3 05 70        			[P_IOB_Data] = R1;
00009359 05 92              			R1 = [BP+5];
0000935A 19 D3 08 70        			[P_IOB_Attrib] = R1;
                            			
0000935C 88 90              			POP R1,R1 FROM [SP];
0000935D 88 98              	        POP BP,BP FROM [SP];
0000935E 90 9A              	        RETF;
                            			.ENDP
                            	
                            	
                            	
                            	_SP_Import: .PROC
0000935F 88 DA              	        PUSH BP,BP TO [SP];
00009360 08 0B 01 00        	        BP = SP + 1;
00009362 90 D4              			PUSH R1,R2 TO [SP];
00009363 03 92              			R1 = [BP+3]; 						// Port Number
00009364 04 94              			R2 = [BP+4]; 						// Data Buffer Pointer
00009365 C1 92              			R1 = [R1] ;
00009366 C2 D2              			[R2] = R1 ;
00009367 90 90              			POP R1,R2 FROM [SP];
00009368 88 98              	        POP BP,BP FROM [SP];
00009369 90 9A              	        RETF;
                            			.ENDP
                            	
                            	_SP_Export: .PROC
0000936A 88 DA              	        PUSH BP,BP TO [SP];
0000936B 08 0B 01 00        	        BP = SP + 1;
0000936D 90 D4              			PUSH R1,R2 TO [SP];
0000936E 03 92              			R1 = [BP+3]; 						// Port Number
0000936F 04 94              			R2 = [BP+4]; 						// Value
00009370 C1 D4              			[R1] = R2 ;
00009371 90 90              			POP R1,R2 FROM [SP];
00009372 88 98              	        POP BP,BP FROM [SP];
00009373 90 9A              	        RETF;        
                            	 		.ENDP
                            			 
                            	
                            	
                            		 
