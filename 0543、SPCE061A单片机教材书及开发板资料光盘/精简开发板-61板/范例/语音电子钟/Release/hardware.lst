000093AB 11 93 16 70        	                r1=[P_DAC2];
000093AD 09 B3 C0 FF        	                r1 &= ~0x003f;
000093AF 09 43 00 80        	                cmp     r1,0x8000
000093B1 0E 0E              	                jb     	L_RU_NormalUp_;  
000093B2 5D 5E              	                je      L_RU_End;
                            	                
                            	L_RU_DownLoop_:
000093B3 40 F0 E0 93        	                call    F_Delay;        
000093B5 41 94              	                r2 = 0x0001;
000093B6 1A D5 12 70        	                [P_Watchdog_Clear] = r2;
000093B8 09 23 40 00        	                r1 -= 0x40;
000093BA 19 D3 16 70        	                [P_DAC2] = r1;
000093BC 09 43 00 80        	                cmp     r1,0x8000;
000093BE 4C 4E              	                jne     L_RU_DownLoop_;  
                            	L_RD_DownEnd_:
000093BF 0C EE              	                jmp     L_RU_End_;
                            	
                            	L_RU_NormalUp_:
                            	L_RU_Loop_:
000093C0 40 F0 E0 93        	                call    F_Delay;
000093C2 41 94              	                r2 = 0x0001;
