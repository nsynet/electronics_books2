                            	L_RQ_QueueEmpty:
                            			//r2 = 0x8000;						// queue empty
00008B80 90 9A              			retf;
                            	///////////////////////////////////////////
                            	// Function: Put a data to Queue
                            	// R1: Input
                            	// Destory: R1,R2,R3
                            	///////////////////////////////////////////	
                            				
                            	F_SP_WriteQueue:
00008B81 12 95 E7 01        			R2 = [R_WriteIndex];			// put data to queue
00008B83 0A 05 B4 01        			R2 += R_Queue;
