                            	///////////////////////////////////////////	
                            	
                            	F_SP_InitQueue:
00008B62 09 93 B4 01        			R1 = R_Queue;
00008B64 40 94              			R2 = 0;
                            	L_ClearQueueLoop:		
00008B65 D1 D4              			[R1++] = R2;
00008B66 09 43 E6 01        			cmp	R1, R_Queue+C_QueueSize;
00008B68 44 4E              			jne	L_ClearQueueLoop;
                            				
00008B69 40 92              			R1 = 0;
00008B6A 19 D3 E6 01        			[R_ReadIndex] = R1;
