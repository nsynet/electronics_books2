00008B9B 11 93 E6 01        			R1 = [R_ReadIndex];
00008B9D 41 22              			R1 -=1;
00008B9E 11 43 E7 01        			cmp R1,[R_WriteIndex];
00008BA0 02 5E              			je	L_TQ_QueueFull;	
                            	
00008BA1 40 92              			r1 = 0;							// not Full, not empty
00008BA2 90 9A              			retf;
                            	L_TQ_QueueFull:
00008BA3 41 92              			r1 = 1;							// full
00008BA4 90 9A              			retf;
                            	L_TQ_QueueEmpty:
