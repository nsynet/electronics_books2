                            	_SP_RampUpDAC1:	.PROC
                            	F_SP_RampUpDAC1:
000080CB 90 D4              	                push r1,r2 to [sp];
000080CC 11 93 17 70        	                r1=[P_DAC1];
000080CE 09 B3 C0 FF        	                r1 &= ~0x003f;
000080D0 09 43 00 80        	                cmp     r1,0x8000
000080D2 0E 0E              	                jb     	L_RU_NormalUp;  
000080D3 19 5E              	                je      L_RU_End;
                            	                
                            	L_RU_DownLoop:
000080D4 40 F0 37 81        	                call    F_Delay;        
000080D6 41 94              	                r2 = 0x0001;
000080D7 1A D5 12 70        	                [P_Watchdog_Clear] = r2;
000080D9 09 23 40 00        	                r1 -= 0x40;
000080DB 19 D3 17 70        	                [P_DAC1] = r1;
000080DD 09 43 00 80        	                cmp     r1,0x8000;
000080DF 4C 4E              	                jne     L_RU_DownLoop;  
                            	L_RD_DownEnd:
000080E0 0C EE              	                jmp     L_RU_End;
                            	
                            	L_RU_NormalUp:
                            	L_RU_Loop:
000080E1 40 F0 37 81        	                call    F_Delay;
000080E3 41 94              	                r2 = 0x0001;
000080E4 1A D5 12 70        	                [P_Watchdog_Clear] = r2;
000080E6 09 03 40 00        	                r1 += 0x40;
000080E8 19 D3 17 70        	                [P_DAC1] = r1;
000080EA 09 43 00 80        	                cmp     r1, 0x8000;
000080EC 4C 4E              	                jne     L_RU_Loop;
                            	                
                            	
                            	L_RU_End:
000080ED 90 90              	                pop     r1,r2 from [sp];
000080EE 90 9A              	                retf;
                            	                .ENDP
