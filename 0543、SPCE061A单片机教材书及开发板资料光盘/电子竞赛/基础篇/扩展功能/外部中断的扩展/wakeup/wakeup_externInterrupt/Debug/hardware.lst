                            	_SP_Init_IOA: .PROC
00008096 88 DA              			PUSH BP,BP TO [SP];
00008097 08 0B 01 00        	        BP = SP + 1;
00008099 88 D2              			PUSH R1,R1 TO [SP];
0000809A 03 92              			R1 = [BP+3];						// Port direction
0000809B 19 D3 02 70        			[P_IOA_Dir] = R1;
0000809D 04 92              			R1 = [BP+4];
0000809E 19 D3 00 70        			[P_IOA_Data] = R1;
000080A0 05 92              			R1 = [BP+5];
000080A1 19 D3 03 70        			[P_IOA_Attrib] = R1;
                            			
000080A3 88 90              			POP R1,R1 FROM [SP];
000080A4 88 98              	        POP BP,BP FROM [SP];
000080A5 90 9A              	        RETF;
                            			.ENDP
                            	
                            	////////////////////////////////////////////////////////////////// 
                            	// SP_Inti_IOB
                            	//////////////////////////////////////////////////////////////////    
                            	
                            	_SP_Init_IOB: .PROC
000080A6 88 DA              			PUSH BP,BP TO [SP];
000080A7 08 0B 01 00        	        BP = SP + 1;
000080A9 88 D2              			PUSH R1,R1 TO [SP];
000080AA 03 92              			R1 = [BP+3];						// Port direction
000080AB 19 D3 07 70        			[P_IOB_Dir] = R1;
000080AD 04 92              			R1 = [BP+4];
000080AE 19 D3 05 70        			[P_IOB_Data] = R1;
000080B0 05 92              			R1 = [BP+5];
000080B1 19 D3 08 70        			[P_IOB_Attrib] = R1;
                            			
000080B3 88 90              			POP R1,R1 FROM [SP];
000080B4 88 98              	        POP BP,BP FROM [SP];
000080B5 90 9A              	        RETF;
                            			.ENDP
                            	
                            	
                            	
                            	_SP_Import: .PROC
000080B6 88 DA              	        PUSH BP,BP TO [SP];
000080B7 08 0B 01 00        	        BP = SP + 1;
000080B9 90 D4              			PUSH R1,R2 TO [SP];
000080BA 03 92              			R1 = [BP+3]; 						// Port Number
000080BB 04 94              			R2 = [BP+4]; 						// Data Buffer Pointer
000080BC C1 92              			R1 = [R1] ;
000080BD C2 D2              			[R2] = R1 ;
000080BE 90 90              			POP R1,R2 FROM [SP];
000080BF 88 98              	        POP BP,BP FROM [SP];
000080C0 90 9A              	        RETF;
                            			.ENDP
                            	
                            	_SP_Export: .PROC
000080C1 88 DA              	        PUSH BP,BP TO [SP];
000080C2 08 0B 01 00        	        BP = SP + 1;
000080C4 90 D4              			PUSH R1,R2 TO [SP];
000080C5 03 92              			R1 = [BP+3]; 						// Port Number
000080C6 04 94              			R2 = [BP+4]; 						// Value
000080C7 C1 D4              			[R1] = R2 ;
000080C8 90 90              			POP R1,R2 FROM [SP];
000080C9 88 98              	        POP BP,BP FROM [SP];
000080CA 90 9A              	        RETF;        
                            	 		.ENDP
                            			 
                            	
                            	
                            		 
