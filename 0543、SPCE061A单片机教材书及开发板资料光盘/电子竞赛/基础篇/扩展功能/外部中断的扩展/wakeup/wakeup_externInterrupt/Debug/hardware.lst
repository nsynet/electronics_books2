                            	L_RD_Loop:                
000080F5 40 F0 37 81        	                call    F_Delay;        
000080F7 41 94              	                r2 = 0x0001;
000080F8 1A D5 12 70        	                [P_Watchdog_Clear] = r2;
000080FA 09 23 40 00        	                r1 -= 0x40;
000080FC 19 D3 17 70        	                [P_DAC1] = r1;  
000080FE 4A 4E              	                jnz     L_RD_Loop;
                            	L_RD_End:       
                            	                
000080FF 90 90              	                pop     r1,r2 from [sp];
00008100 90 9A              	                retf;
                            					.ENDP
                            	
