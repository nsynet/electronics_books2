                            	F_SP_ReadQueue_NIC_A2000:
                            	F_SP_ReadQueue_NIC_S480:
                            	F_SP_ReadQueue_NIC_S240:
                            	F_SP_ReadQueue_NIC_MS01:
                            	F_SP_ReadQueue_NIC_DVR:
0000C538 12 95 ED 02        			R2 = [R_ReadIndex]
0000C53A 12 45 EE 02        			cmp R2,[R_WriteIndex]
0000C53C 03 5E              			je	L_RQ_QueueEmpty?
                            	
0000C53D 0A 05 5D 02        			R2 += R_Queue				// get queue data index
0000C53F C2 92              			R1 = [R2]
                            	L_RQ_QueueEmpty?:
0000C540 90 9A              			RETF
                            			
                            	///////////////////////////////////////////
                            	// Function: Put a data to Queue
                            	// R1: Input
                            	// Destory: R1,R2
                            	///////////////////////////////////////////	
                            	F_SP_WriteQueue_A2000:
                            	F_SP_WriteQueue_S480:
                            	F_SP_WriteQueue_S240:
                            	F_SP_WriteQueue_MS01:
                            	F_SP_WriteQueue_DVR:
                            	F_SP_WriteQueue:
0000C541 12 95 EE 02        			R2 = [R_WriteIndex] 			// put data to queue
0000C543 0A 05 5D 02        			R2 += R_Queue
0000C545 C2 D2              			[R2] = R1
                            	
0000C546 12 95 EE 02        			R2 = [R_WriteIndex]
0000C548 41 04              			R2 += 1
0000C549 0A 45 90 00        			cmp	R2, C_QueueSize
0000C54B 01 4E              			jne	L_WQ_NotQueueBottom
0000C54C 40 94              			R2 = 0
                            	L_WQ_NotQueueBottom:
0000C54D 1A D5 EE 02        			[R_WriteIndex] = R2
0000C54F 90 9A              			RETF
                            	
                            			
