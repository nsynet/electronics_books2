                            	////////////////////////////////////////////////////////////////// 
                            	// Function: Get data from port
                            	// 	int SP_Import(unsigned int Port)
                            	//////////////////////////////////////////////////////////////////  
                            	_SP_Import: .PROC
0000C6D0 88 DA              	        PUSH BP TO [SP] 
0000C6D1 08 0B 01 00        	        BP = SP + 1 
                            	        
0000C6D3 03 92              			R1 = [BP+3]  						// Port Number
0000C6D4 C1 92              			R1 = [R1]  
                            					
0000C6D5 88 98              	        POP BP FROM [SP] 
0000C6D6 90 9A              	        RETF 
                            			.ENDP
                            	
                            	_SP_Export: .PROC
0000C6D7 88 DA              	        PUSH BP,BP TO [SP] 
0000C6D8 08 0B 01 00        	        BP = SP + 1 
