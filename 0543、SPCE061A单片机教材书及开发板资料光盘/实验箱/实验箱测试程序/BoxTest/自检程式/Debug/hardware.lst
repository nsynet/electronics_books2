0000C55F 41 22              			R1 -=1 
0000C560 11 43 EE 02        			cmp R1,[R_WriteIndex] 
0000C562 02 5E              			je	L_TQ_QueueFull 	
                            	
0000C563 40 92              			r1 = 0 							// not Full, not empty
0000C564 90 9A              			retf 
