0000C6BA 05 92              			R1 = [BP+5] 
0000C6BB 19 D3 03 70        			[P_IOA_Attrib] = R1 
                            			
0000C6BD 88 90              			POP R1 FROM [SP] 
0000C6BE 88 98              	        POP BP FROM [SP] 
0000C6BF 90 9A              	        RETF 
                            			.ENDP
                            	
