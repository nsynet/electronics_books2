                            	////////////////////////////////////////////////////////
                            	// Function: Ramp Up/Down to avoid speaker "pow" noise
                            	// Destory: R1,R2
                            	////////////////////////////////////////////////////////
                            	_SP_RampUpDAC1:	.PROC
                            	F_SP_RampUpDAC1:
0000C63E 90 D4              			push r1,r2 to [sp] 
0000C63F 11 93 17 70        	        r1=[P_DAC1] 
0000C641 09 B3 C0 FF        	        r1 &= ~0x003f 
0000C643 09 43 00 80        	        cmp     r1,0x8000
0000C645 0E 0E              	        jb     	L_RU_NormalUp
0000C646 19 5E              	        je      L_RU_End
                            	                
                            	L_RU_DownLoop:
0000C647 40 F0 AA C6        	        call    F_Delay         
0000C649 41 94              	        r2 = 0x0001 
0000C64A 1A D5 12 70        	        [P_Watchdog_Clear] = r2 
0000C64C 09 23 40 00        	        r1 -= 0x40 
0000C64E 19 D3 17 70        	        [P_DAC1] = r1 
0000C650 09 43 00 80        	        cmp     r1,0x8000 
0000C652 4C 4E              	        jne     L_RU_DownLoop   
                            	L_RD_DownEnd:
0000C653 0C EE              	        jmp     L_RU_End 
                            	
                            	L_RU_NormalUp:
                            	L_RU_Loop:
0000C654 40 F0 AA C6        	        call    F_Delay 
0000C656 41 94              	        r2 = 0x0001 
0000C657 1A D5 12 70        	        [P_Watchdog_Clear] = r2 
0000C659 09 03 40 00        	        r1 += 0x40 
0000C65B 19 D3 17 70        	        [P_DAC1] = r1 
0000C65D 09 43 00 80        	        cmp     r1, 0x8000 
0000C65F 4C 4E              	        jne     L_RU_Loop 
                            	L_RU_End:
0000C660 90 90              			pop     r1,r2 from [sp] 
0000C661 90 9A              	  		retf 
                            	    	.ENDP
                            	    
