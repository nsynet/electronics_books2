0000C6DB 03 92              			R1 = [BP+3]  						// Port Number
0000C6DC 04 94              			R2 = [BP+4]  						// Value
0000C6DD C1 D4              			[R1] = R2  
0000C6DE 90 90              			POP R1,R2 FROM [SP] 
0000C6DF 88 98              	        POP BP,BP FROM [SP] 
0000C6E0 90 9A              	        RETF         
                            	 		.ENDP
                            	
                            	
                            	////////////////////////////////////////////////////////////////// 
                            	// Function: Get data from resource(ROM area)
                            	// 	int SP_GetResource(int Addr, int Page)
                            	//////////////////////////////////////////////////////////////////  
                            	_SP_GetResource:	.PROC
0000C6E1 88 DA              			push bp to [sp] 
0000C6E2 08 0B 01 00        			bp = sp + 1 
                            			
0000C6E4 03 92              			r1 = [bp+3] 					// Address
0000C6E5 04 94              			r2 = [bp+4] 					// Page
                            			
0000C6E6 5A 95              			r2 = r2 lsl 4 					// Prepare Page for SR
0000C6E7 5A 95              			r2 = r2 lsl 4 
0000C6E8 4A 95              			r2 = r2 lsl 2 
                            			
0000C6E9 7F BC              			sr &= 0x03f 					// Change Page
0000C6EA 06 A5              	        r2 |=sr 						//
